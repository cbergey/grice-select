	pages = {229--254},
	file = {Full Text PDF:/Users/clairebergey/Zotero/storage/NT55NTA7/Paraboni et al. - 2007 - Generating Referring Expressions Making Referents.pdf:application/pdf;Snapshot:/Users/clairebergey/Zotero/storage/TL9HKZG3/coli.2007.33.2.html:text/html}
}

@article{gweon_sins_2014,
	title = {Sins of omission: {Children} selectively explore when teachers are under-informative},
