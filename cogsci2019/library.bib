	file = {ScienceDirect Full Text PDF:/Users/clairebergey/Zotero/storage/VK9EJLBD/Au and Markman - 1987 - Acquiring word meanings via linguistic contrast.pdf:application/pdf;ScienceDirect Snapshot:/Users/clairebergey/Zotero/storage/LFRXQ7J3/S0885201487900591.html:text/html}
}

@article{barner_compositionality_2008,
